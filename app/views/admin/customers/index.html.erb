<div class="container">
  <div class="row my-2">
    <h3>会員一覧</h3>
  </div>
  <table class="table">
    <tr>
      <th>会員ID</th>
      <th>氏名</th>
      <th>メールアドレス</th>
      <th>ステータス</th>
    </tr>
    <% @customers.each do |customer| %>
    <tr>
      <td><%= customer.id %></td>
      <td><%= customer.fullname %></td>
      <td><%= customer.email %></td>
      <td><%= customer.is_active %></td>
    </tr>
    <% end %>
  </table>